{
	"name": "renraku",
	"version": "0.0.0-dev.3",
	"description": "node json rpc",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com",
	"main": "dist/cjs/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean barrel && run-p compile-esm compile-cjs",
		"clean": "rimraf dist && mkdirp dist dist/cjs",
		"barrel": "barrelsby --delete --directory source --exclude '\\.test\\.ts$' && replace --silent '\";$' '.js\"' source/index.ts",
		"compile-esm": "tsc",
		"compile-cjs": "tsc --module commonjs --outDir dist/cjs",
		"start": "node dist/cjs/example/example-server",
		"test": "exit 0"
	},
	"keywords": [
		"rpc",
		"json",
		"node-api",
		"api"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/renraku.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/renraku/issues"
	},
	"homepage": "https://github.com/chase-moskal/renraku#readme",
	"devDependencies": {
		"@types/koa": "^2.0.49",
		"@types/koa__cors": "^2.2.3",
		"@types/node": "^12.7.3",
		"barrelsby": "^2.1.1",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"replace": "^1.1.1",
		"rimraf": "^3.0.0",
		"typescript": "^3.6.2"
	},
	"dependencies": {
		"@koa/cors": "^3.0.0",
		"isomorphic-fetch": "^2.2.1",
		"koa": "^2.8.1",
		"koa-body": "^4.1.1"
	}
}
