{
	"name": "renraku",
	"version": "0.0.0-dev.13",
	"description": "node json rpc",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com",
	"main": "dist/cjs/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile-esm compile-cjs",
		"clean": "rimraf dist && mkdirp dist dist/cjs",
		"compile-esm": "tsc",
		"compile-cjs": "tsc --module commonjs --outDir dist/cjs",
		"start-api": "node -e 'require(\"./dist/cjs/example/example-server\").exampleServer()'",
		"start-http": "serve -l 8000",
		"start": "run-p start-api start-http",
		"test": "exit 0"
	},
	"keywords": [
		"rpc",
		"json",
		"node-api",
		"api"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/renraku.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/renraku/issues"
	},
	"homepage": "https://github.com/chase-moskal/renraku#readme",
	"devDependencies": {
		"@types/isomorphic-fetch": "0.0.35",
		"@types/koa": "^2.0.51",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa__cors": "^2.2.3",
		"@types/node": "^12.11.7",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"replace": "^1.1.1",
		"rimraf": "^3.0.0",
		"serve": "^11.2.0",
		"typescript": "^3.6.4"
	},
	"dependencies": {
		"@koa/cors": "^3.0.0",
		"isomorphic-fetch": "^2.2.1",
		"koa": "^2.10.0",
		"koa-bodyparser": "^4.2.1"
	}
}
