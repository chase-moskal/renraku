{
	"name": "renraku",
	"version": "0.0.0-dev.20",
	"description": "web api library",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com",
	"main": "dist-cjs/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"source",
		"dist-cjs"
	],
	"scripts": {
		"prepare": "run-s clean && run-p compile-esm compile-cjs generate-keys && run-s test",
		"clean": "rimraf dist && mkdirp dist dist-cjs",
		"compile-esm": "tsc",
		"compile-cjs": "tsc --module commonjs --outDir dist-cjs",
		"start-api": "node -e 'require(\"./dist-cjs/internals/example/example-server\").exampleServer()'",
		"start-http": "serve -l 8000",
		"start": "run-p start-api start-http",
		"generate-keys": "./generate-keys",
		"test": "jest --verbose dist-cjs"
	},
	"keywords": [
		"rpc",
		"json",
		"api",
		"web-api",
		"node-api"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chase-moskal/renraku.git"
	},
	"bugs": {
		"url": "https://github.com/chase-moskal/renraku/issues"
	},
	"homepage": "https://github.com/chase-moskal/renraku#readme",
	"devDependencies": {
		"@types/isomorphic-fetch": "0.0.35",
		"@types/jest": "^24.9.0",
		"@types/node": "^13.1.7",
		"jest": "^24.9.0",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"replace": "^1.1.5",
		"rimraf": "^3.0.0",
		"serve": "^11.3.0",
		"typescript": "^3.7.4"
	},
	"dependencies": {
		"@koa/cors": "^3.0.0",
		"@types/koa": "^2.11.0",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa__cors": "^3.0.1",
		"isomorphic-fetch": "^2.2.1",
		"koa": "^2.11.0",
		"koa-bodyparser": "^4.2.1"
	}
}
